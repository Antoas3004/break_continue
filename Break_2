public class Break_2 {

	public static void main(String[] args) {
		// break for double loop
		// break in inner loop

		for (int i = 1; i <= 5; ++i) {
			for (int j = 1; j <= 5; ++j) {
				if (i == 2 && j == 2) {
					// in this case loop stop in inner loop and again start for
					// outer loop because outer loop wont consider break statement
					break;
				}
				System.out.println(i + " " + j);
			}
		}
	}
}
