public class continue_3 {

	public static void main(String[] args) {

            //double for loop
		        //continue statement in outer for loop

     outer: for (int i = 1; i <= 5; ++i) {
			inner: for (int j = 1; j <= 5; ++j) {
				if (i == 2 && j == 2) {
					continue outer;
				}
				System.out.println(i + " " + j);
			}
		}
	}
}
