public class break_3 {

	public static void main(String[] args) {
		// break statement for outer loop
		// we use break for outer for loops using  labeled
		

		outer: for (int i = 1; i <= 5; i++) {
			inner: for (int j = 1; j <= 5; j++) {
				if (i == 2 && j == 2) {
					break outer;
				}
				System.out.println(i + " " + j);
			}
		}
	}
}
