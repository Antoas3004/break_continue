public class continue_2 {

	public static void main(String[] args) {
		// double for loop
		// use continue statement in inner loop
		
		for(int i=1;i<=5;i++) {
			for(int j=1;j<=5;j++) {
				if(i==3 || j==3) {
					continue;
					//skip the current iteration
				}
				System.out.println(i+" "+j);
			}
		}
	}
}
